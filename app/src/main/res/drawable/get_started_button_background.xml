        <shape android:shape="rectangle">
            <solid android:color="#9c634b"></solid>
            <corners android:radius="35dp"></corners>
        </shape>
        <color android:color="#eadebe"></color>

    </item>

    <item android:state_enabled="true" android:state_pressed="true">
